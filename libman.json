{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "leaflet.marker.slideto@0.3.0",
      "destination": "wwwroot/lib/leaflet.marker.slideto/"
    },
    {
      "library": "@geoman-io/leaflet-geoman-free@2.16.0",
      "destination": "wwwroot/lib/geoman-io/leaflet-geoman-free/"
    },
    {
      "library": "leaflet-easybutton@2.4.0",
      "destination": "wwwroot/lib/leaflet-easybutton/",
      "files": [
        "src/easy-button.js",
        "src/easy-button.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "luxon@3.4.4",
      "destination": "wwwroot/lib/luxon/",
      "files": [
        "luxon.min.js.map",
        "luxon.js",
        "luxon.js.map",
        "luxon.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "Chart.js@4.4.1",
      "destination": "wwwroot/lib/chartjs/",
      "files": [
        "chart.cjs.map",
        "chart.js",
        "chart.js.map",
        "chart.min.js",
        "chart.umd.js",
        "chart.umd.js.map",
        "chart.umd.min.js",
        "helpers.cjs.map",
        "helpers.js",
        "helpers.js.map",
        "helpers.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "chartjs-adapter-luxon@1.3.1",
      "destination": "wwwroot/lib/chartjs-adapter-luxon/",
      "files": [
        "chartjs-adapter-luxon.umd.js",
        "chartjs-adapter-luxon.umd.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "chartjs-plugin-datalabels@2.2.0",
      "destination": "wwwroot/lib/chartjs-plugin-datalabels/"
    }
  ]
}